{
  "manifest_version": 3,
  "name": "PhotoLancer",
  "description": "Secure your photos as blur and enjoy",
  "version": "1.0",
  "icons": {
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://photos.google.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": []
}
